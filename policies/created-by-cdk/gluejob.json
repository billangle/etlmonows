{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "s3:GetObject",
                "s3:ListBucket",
                "ssm:GetParameter"
            ],
            "Resource": "*",
            "Effect": "Allow"
        },
        {
            "Action": [
                "s3:Abort*",
                "s3:DeleteObject*",
                "s3:GetBucket*",
                "s3:GetObject*",
                "s3:List*",
                "s3:PutObject",
                "s3:PutObjectLegalHold",
                "s3:PutObjectRetention",
                "s3:PutObjectTagging",
                "s3:PutObjectVersionTagging"
            ],
            "Resource": [
                "arn:aws:s3:::punkdev-fpacfsa-cleansed-zone",
                "arn:aws:s3:::punkdev-fpacfsa-final-zone",
                "arn:aws:s3:::punkdev-fpacfsa-landing-zone",
                "arn:aws:s3:::punkdev-fpacfsa-cleansed-zone/*",
                "arn:aws:s3:::punkdev-fpacfsa-final-zone/*",
                "arn:aws:s3:::punkdev-fpacfsa-landing-zone/*"
            ],
            "Effect": "Allow"
        }
    ]
}